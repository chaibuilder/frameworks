{
  "name": "chai-next",
  "version": "0.0.24-beta",
  "description": "Chai Builder NextJS Package",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "tsup  --watch --format esm,cjs",
    "build": "tsup  --dts --format esm,cjs"
  },
  "bin": {
    "chai-tailwind": "./scripts/chai-tailwind.cjs"
  },
  "keywords": [
    "chai-builder",
    "nextjs",
    "tailwindcss",
    "react",
    "builder",
    "cms"
  ],
  "author": "Suraj Air",
  "email": "suraj@chaibuilder.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "dependencies": {
    "@ai-sdk/openai": "^2.0.18",
    "@chaibuilder/pages": "0.11.10",
    "@chaibuilder/sdk": "2.5.9",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.50.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "ai": "^5.0.20",
    "chalk": "^5.4.1",
    "date-fns": "^4.1.0",
    "lodash": "4.17.20",
    "postcss": "8.5.6",
    "raw-loader": "^4.0.2",
    "sharp": "0.34.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.17"
  },
  "peerDependencies": {
    "next": "^15",
    "react": "^19",
    "react-dom": "^19"
  },
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./blocks": {
      "types": "./dist/blocks/index.d.ts",
      "import": "./dist/blocks/index.js",
      "require": "./dist/blocks/index.cjs"
    },
    "./blocks/rsc": {
      "types": "./dist/blocks/rsc/index.d.ts",
      "import": "./dist/blocks/rsc/index.js",
      "require": "./dist/blocks/rsc/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    },
    "./builder-styles": {
      "import": "./dist/core/index.css",
      "require": "./dist/core/index.css"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.cjs"
    }
  },
  "files": [
    "dist",
    "scripts"
  ],
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/lodash": "4.17.20",
    "@types/node": "^22.15.30",
    "@vitest/coverage-v8": "^1.4.0",
    "tsup": "^8.0.1",
    "typescript": "5.8.2",
    "vitest": "^1.4.0"
  }
}
